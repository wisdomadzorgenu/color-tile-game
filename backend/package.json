{
  "name": "everreal-game-backend",
  "version": "1.0.0",
  "description": "ai game to generate sequences to solution",
  "main": "./build/server.js",
  "scripts": {
    "test": "mocha --require ts-node/register -w src/test/*.test.ts",
    "compile": "tsc && node ./build/server.js",
    "start-server": "nodemon --delay 3 --watch src --ext ts --ignore '*.test.ts' --exec \"npm run compile\" ",
    "start":"node ./build/server.js",
    "build": "tsc"
  },
  "keywords": [
    "everreal",
    "game",
    "ai",
    "play",
    "coding",
    "challenge"
  ],
  "author": "Wisdom Adzorgenu",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.8",
    "@types/mocha": "^8.0.3",
    "@types/supertest": "^2.0.10",
    "chai": "^4.2.0",
    "mocha": "^8.2.1",
    "nodemon": "^2.0.6",
    "supertest": "^6.0.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  }
}
